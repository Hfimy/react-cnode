{
  "name": "react-ssr-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "clear": "rimraf dist",
    "dev:client": "webpack-dev-server --config config/webpack.dev.js",
    "dev:ssr": "cross-env NODE_ENV=development nodemon server/app.js",
    "dev": "npm run clear && start npm run dev:client &&start npm run dev:ssr",
    "build:client": "webpack --config config/webpack.prod.js",
    "build:ssr": "webpack --config config/webpack.ssr.js",
    "build": "npm run clear && npm run build:client && npm run build:ssr",
    "start": "nodemon server/app.js"
  },
  "pre-commit": [
    "fix",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hfimy/react-ssr-template.git"
  },
